{
  "name": "aws-bot",
  "version": "1.0.0",
  "description": "Your friendly neighborhood robot",
  "scripts": {
    "build": "yarn run clean && webpack --config webpack.prod.config.js",
    "start": "node ./dist/main.bundle.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec node_modules/.bin/ts-node ./src/app.ts",
    "clean": "rimraf dist",
    "lint": "eslint \"./src/**/*.{js,ts}\" --fix --cache",
    "test": "yarn run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erinknight242/aws-bot.git"
  },
  "keywords": [
    "bolt",
    "slack"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Erin Knight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erinknight242/aws-bot/issues"
  },
  "homepage": "https://github.com/erinknight242/aws-bot#readme",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.282.0",
    "@slack/bolt": "^3.12.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "aws-crt": "^1.15.9",
    "babel-loader": "^9.1.2",
    "bufferutil": "^4.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "utf-8-validate": "^6.0.3",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1",
    "webpack-merge": "^5.8.0"
  }
}
